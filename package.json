{
  "name": "crud-koa-example",
  "version": "1.0.0",
  "description": "A crud with koa and mongodb",
  "main": "app.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/italosestilon/crud-koa-example.git"
  },
  "dependencies": {
    "config": "^2.0.1",
    "dataloader": "^1.4.0",
    "graphql": "^14.0.2",
    "koa": "^2.5.3",
    "koa-graphql": "^0.7.5",
    "mongoose": "^5.2.17",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^5.6.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^2.7.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "nodemon": "^1.18.4",
    "prettier": "1.14.3"
  },
  "scripts": {
    "babel": "nodemon --exec babel-node app.js",
    "pretest": "eslint --ignore-path .gitignore .",
    "test": "jest --runInBand",
    "prettier-watch": "onchange '**/*.js' -- prettier --use-tabs --write {{changed}}"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "./dist"
    ],
    "coverageReporters": [
      "lcov",
      "html"
    ],
    "moduleNameMapper": {
      "^mongoose$": "<rootDir>/node_modules/mongoose"
    }
  }
}
